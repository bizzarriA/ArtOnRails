<p id="notice"><%= notice %></p>

<h1>Artista</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artista.each do |artistum| %>
      <tr>
        <td><%= link_to 'Show', artistum %></td>
        <td><%= link_to 'Edit', edit_artistum_path(artistum) %></td>
        <td><%= link_to 'Destroy', artistum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artistum', new_artistum_path %>
